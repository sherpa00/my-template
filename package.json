{
  "name": "my_template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=testing jest --verbose",
    "test:unit": "cross-env NODE_ENV=testing jest --verbose --testMatch='**/src/__tests__/unit/*.test.[jt]s?(x)",
    "test:integration": "cross-env NODE_ENV=testing jest --verbose --testMatch='**/src/__tests__/integration/*.test.[jt]s?(x)",
    "dev": "cross-env NODE_ENV=development nodemon --exec ts-node src/server.ts",
    "start": "cross-env NODE_ENV=production node build/server",
    "build": "tsc",
    "lint": "eslint . --ext .ts --fix",
    "format": "npx prettier --ignore-path .gitignore --write ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.6",
    "@types/morgan": "^1.9.7",
    "@types/node": "^20.8.8",
    "@types/supertest": "^2.0.15",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "pretty-quick": "^3.1.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
